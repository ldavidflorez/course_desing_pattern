[run]
source = .
omit =
    */tests/*
    */test_*
    */__pycache__/*
    */venv/*
    */htmlcov/*
    */.pytest_cache/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov